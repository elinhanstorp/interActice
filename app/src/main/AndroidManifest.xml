<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.elin.interactice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WorkoutActivity" />

        <activity android:name=".DistanceActivity"
            android:parentActivityName=".WorkoutActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".WorkoutActivity" /></activity>

        <activity android:name=".JumpActivity"
        android:parentActivityName=".WorkoutActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".WorkoutActivity" /></activity>

        <activity android:name=".PushUpActivity"
        android:parentActivityName=".WorkoutActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".WorkoutActivity" /></activity>

        <activity android:name=".FinishedActivity"
            android:parentActivityName=".WorkoutActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".WorkoutActivity" /></activity>
    </application>

</manifest>

